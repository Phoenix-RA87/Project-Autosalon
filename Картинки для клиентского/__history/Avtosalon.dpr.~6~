  Application.CreateForm(TRAmenu, RAmenu);
  Application.CreateForm(TRAVDog, RAVDog);
  Application.CreateForm(TRADM, RADM);
  Application.CreateForm(TRAAVTO, RAAVTO);
  Application.CreateForm(TRAVPokup, RAVPokup);
  Application.CreateForm(TRAPDog, RAPDog);
  Application.CreateForm(TRAVReklamac, RAVReklamac);
  Application.CreateForm(TRAKAVTO, RAKAVTO);
